{
  "name": "open-weather-graph",
  "author": "swansswansswansswanssosoft",
  "version": "1.5.0",
  "keywords": [
    "pebble-app"
  ],
  "private": true,
  "dependencies": {
    "message-queue-pebble": "^1.0.0",
    "pebble-battery-bar": "^1.0.10",
    "pebble-bluetooth-icon": "^1.0.8",
    "pebble-clay": "^1.0.4",
    "pebble-moon-layer": "^1.2.1"
  },
  "pebble": {
    "displayName": "OpenWeatherGraph",
    "capabilities": [
      "geolocation",
      "configurable"
    ],
    "uuid": "24c4e9b1-dc28-48ad-be60-67edcf780d55",
    "sdkVersion": "3",
    "enableMultiJS": true,
    "targetPlatforms": [
      "aplite",
      "basalt",
      "diorite"
    ],
    "watchapp": {
      "watchface": true
    },
    "messageKeys": {
      "APIkey": 1,
      "APIchoice": 2,
      "GraphTemperature": 100,
      "GraphCloudCover": 101,
      "GraphPrecipType": 102,
      "GraphPrecipProb": 103,
      "GraphHumidity": 104,
      "GraphPressure": 105,
      "GraphWindSpeed": 106,
      "DailyHighs": 110,
      "DailyLows": 111,
      "DayMarkers": 112,
      "DaysOfTheWeek": 113,
      "CurrentTemperature": 114,
      "GetWeather": 115
    },
    "resources": {
      "media": [
        {
          "file": "fonts/type_writer.ttf",
          "name": "FONT_TYPE_WRITER_8",
          "targetPlatforms": null,
          "type": "font"
        }
      ]
    }
  }
}
